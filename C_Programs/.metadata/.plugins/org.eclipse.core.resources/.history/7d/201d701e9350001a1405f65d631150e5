#include<stdio.h>
#include<string.h>
char *copy_string(char *,char *);
char compare_string(char [],char[]);
char *Accept_string();
int main(void)
{
	char *str1;
	char *str2;

	printf("\n-------------strcpy()--------------");
	str1=Accept_string();
	str2=Accept_string();
	printf("%s",str1);
	copy_string(str1,str2);

	//printf("\n------------strcmp()------------");

	//compare_string(str1,str2);



	return 0;
}
char *Accept_string()
{
	char *str;
	printf("\nEnter string");
	scanf("%s",str);
	return str;

}
char *copy_string(char *s1,char *s2)
{
	int i;
	for(i=0;s2[i]!='\0';i++)
   {
	s1[i]=s2[i];

   }
	s1[i]='\0';
	return s1;
}
