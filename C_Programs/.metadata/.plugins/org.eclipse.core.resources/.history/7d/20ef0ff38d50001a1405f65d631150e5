void Accept_string(char *str)
{
	printf("\nEnter string");
	scanf("%s",str);


}
void copy_string(char *s1[20],char *s2[20])
{
	int i;
	for(i=0;i!='\0';i++)
   {
	*s1[i]=*s2[i];

   }
	//return &s1;
}
