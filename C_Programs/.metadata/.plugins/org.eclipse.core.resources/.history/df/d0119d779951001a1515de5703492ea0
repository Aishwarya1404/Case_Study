#include<stdio.h>

void accept_string(char *str,char ch)
{
	printf("\nEnter string");
	scanf("%s",str);
	//printf("\nEnter character");
	//scanf("%c",&ch);
}
void  my_strtok(char str[],char ch)
{
	int i;
	for(i=0;str[i]!='\0';i++)
	{
		if(str[i]!=ch)
		{
			printf("%c",str[i]);
			continue;
		}
		else
		{
			printf("\n");
			continue;
		}
	}
}

int main(void)
{
	char str[50];
	char ch;
	accept_string(str,' ');
	my_strtok(str,ch);
	//printf("\nString is:%s",str);
	return 0;
}
