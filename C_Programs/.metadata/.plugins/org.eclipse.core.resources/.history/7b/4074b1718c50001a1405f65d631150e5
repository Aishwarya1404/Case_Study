char copy_string(char [],char[]);
char compare_string(char [],char[]);
char Accept_string();
int main(void)
{
	char str1[20];
	char str2[20];

	printf("\n-------------strcpy()--------------");
	str1[20]=Accept_string();
	str2[20]=Accept_string();
	copy_string(str1,str2);
	printf("%s",str1);
	//printf("\n------------strcmp()------------");

	//compare_string(str1,str2);



	return 0;
}
char Accept_string()
{
	char str[20];
	printf("\nEnter string");
	scanf("%s",str);
	return str;

}
char copy_string(char s1[],char s2[])
{
	int i;
	for(i=0;i<strlen(s2);i++)
   {
	s1[i]=s2[i];

   }
	return s1;
}
